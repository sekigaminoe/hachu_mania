<h2>ユーザー一覧</h2>

<div class="contaner">
  <div class="row">
    <% @users.each do |user| %>
      <%= image_tag user.get_profile_image %>
      <%= link_to user_path(user) do %>
        <%= user.name %>
      <% end %>
    <% end %>
  </div>
  <%= link_to "グループを作成する", new_group_path, class: "btn btn-success" %>
  <%= link_to "グループ一覧", groups_path, class: "btn btn-primary" %>
</div>