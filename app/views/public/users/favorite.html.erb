<h2>いいね一覧</h2>
<table class="table">
  <thead>
    <tr>
      <th>投稿写真</th>
      <th>タイトル</th>
      <th>ユーザー名</th>
    </tr>
  </thead>
  <tbody>
    <% @favorite_posts.each do |post| %>
    <tr>
      <td><%= image_tag post.get_post_image %></td>
      <td><%= post.title %></td>
      <td><%= link_to post.user %></td>
    </tr>
    <% end %>
  </tbody>
</table>